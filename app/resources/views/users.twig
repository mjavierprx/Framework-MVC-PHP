{% extends "layout.html" %}
{% block title %}
    Listado usuarios con Twig
{% endblock %}
{% block content %}
    <h1>Listado de usuarios:</h1>
    <ul>
        {% for user in users %}
            <li>{{ user.name }} - {{ user.email }}</li>
        {% else %}
            No se encontraron usuarios.
        {% endfor %}
    </ul>
{% endblock %}